{
    "name": "bramato/imag",
    "description": ":package_description",
    "keywords": [
        "bramato",
        "imag"
    ],
    "homepage": "https://github.com/bramato/imag",
    "license": "MIT",
    "authors": [
        {
            "name": "Bramato Marco",
            "email": "marco@bramato.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "eddturtle/direct-upload": "^3.0",
        "illuminate/database": "^8.80",
        "illuminate/support": "^8.80",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "laravel/helpers": "^1.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "bramato\\imag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bramato\\imag\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
